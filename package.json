{
  "name": "preact-ssr",
  "version": "0.0.0",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "vite build --config vite.config.js",
    "build:watch": "yarn build -w",
    "dev": "run-p build:watch wrangler",
    "wrangler": "wrangler dev --local --live-reload ./src/server.tsx",
    "deploy": "wrangler publish src/server.tsx"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "vite": "^4.0.3",
    "wrangler": "^2.6.2"
  },
  "dependencies": {
    "hono": "3.0.0-rc.4",
    "preact": "^10.11.3",
    "preact-render-to-string": "^5.2.6"
  }
}
